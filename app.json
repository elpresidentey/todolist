{
  "expo": {
    "name": "TODO",
    "slug": "todo-app",
    "scheme": "todoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "android": {
      "package": "com.hng.todoapp"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hng.todoapp"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2cd0cef4-24a6-4314-882f-b9a5ceb94617"
      }
    }
  }
}
